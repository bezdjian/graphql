type Link {
    url: String!
    description: String!
}

type Person {
    name: String!
    job: String!
    address: String!
}

type Query {
    allLinks: [Link]
    allPersons: [Person]
}

type Mutation {
    createLink(url: String!, description: String!): Link
    #createPerson(name: String!, job: String!, address: Address!)
}

schema {
    query: Query
    mutation: Mutation
}